#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/nxtwave/reactjs/sessions/Introduction-to-React-JS/reachinbox-onebox/node_modules/.pnpm/acorn@8.15.0/node_modules/acorn/bin/acorn/node_modules:/home/nxtwave/reactjs/sessions/Introduction-to-React-JS/reachinbox-onebox/node_modules/.pnpm/acorn@8.15.0/node_modules/acorn/bin/node_modules:/home/nxtwave/reactjs/sessions/Introduction-to-React-JS/reachinbox-onebox/node_modules/.pnpm/acorn@8.15.0/node_modules/acorn/node_modules:/home/nxtwave/reactjs/sessions/Introduction-to-React-JS/reachinbox-onebox/node_modules/.pnpm/acorn@8.15.0/node_modules:/home/nxtwave/reactjs/sessions/Introduction-to-React-JS/reachinbox-onebox/node_modules/.pnpm/node_modules:/home/nxtwave/reactjs/sessions/Introduction-to-React-JS/reachinbox-onebox/node_modules:/home/nxtwave/reactjs/sessions/Introduction-to-React-JS/node_modules:/home/nxtwave/reactjs/sessions/node_modules:/home/nxtwave/reactjs/node_modules:/home/nxtwave/node_modules:/home/node_modules:/node_modules:/home/nxtwave/reactjs/sessions/Introduction-to-React-JS/reachinbox-onebox/node_modules/.pnpm/acorn-walk@8.3.4/node_modules/acorn-walk/node_modules:/home/nxtwave/reactjs/sessions/Introduction-to-React-JS/reachinbox-onebox/node_modules/.pnpm/acorn-walk@8.3.4/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../acorn@8.15.0/node_modules/acorn/bin/acorn" "$@"
else 
  exec node  "$basedir/../../../../../acorn@8.15.0/node_modules/acorn/bin/acorn" "$@"
fi
