#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/nxtwave/reactjs/sessions/Introduction-to-React-JS/reachinbox-onebox/node_modules/.pnpm/typescript@5.9.3/node_modules/typescript/bin/tsserver/node_modules:/home/nxtwave/reactjs/sessions/Introduction-to-React-JS/reachinbox-onebox/node_modules/.pnpm/typescript@5.9.3/node_modules/typescript/bin/node_modules:/home/nxtwave/reactjs/sessions/Introduction-to-React-JS/reachinbox-onebox/node_modules/.pnpm/typescript@5.9.3/node_modules/typescript/node_modules:/home/nxtwave/reactjs/sessions/Introduction-to-React-JS/reachinbox-onebox/node_modules/.pnpm/typescript@5.9.3/node_modules:/home/nxtwave/reactjs/sessions/Introduction-to-React-JS/reachinbox-onebox/node_modules/.pnpm/node_modules:/home/nxtwave/reactjs/sessions/Introduction-to-React-JS/reachinbox-onebox/node_modules:/home/nxtwave/reactjs/sessions/Introduction-to-React-JS/node_modules:/home/nxtwave/reactjs/sessions/node_modules:/home/nxtwave/reactjs/node_modules:/home/nxtwave/node_modules:/home/node_modules:/node_modules:/home/nxtwave/reactjs/sessions/Introduction-to-React-JS/reachinbox-onebox/node_modules/.pnpm/ts-node@10.9.2_0b66c5a3f25a04de190d9e861ab43d62/node_modules/ts-node/node_modules:/home/nxtwave/reactjs/sessions/Introduction-to-React-JS/reachinbox-onebox/node_modules/.pnpm/ts-node@10.9.2_0b66c5a3f25a04de190d9e861ab43d62/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../typescript@5.9.3/node_modules/typescript/bin/tsserver" "$@"
else 
  exec node  "$basedir/../../../../../typescript@5.9.3/node_modules/typescript/bin/tsserver" "$@"
fi
